    "version": "0.2.0",
    "configurations": [ 
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "debug"
            ],
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ssr",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin/box.js",
            "args": ["prod_ssr","-r"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ssr_server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin/box.js",
            "args": ["ssr_server","-r"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dll",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin/box.js",
            "args": ["dll","-r"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "prod",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin/box.js",
            "args": ["prod", "react", "-m", "-r", "-g"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dev",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin/box.js",
            "args": ["dev","react","-m"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\scripts/dev.js",    
        }
    ]
}